{
    "name": "xiongchao/shell-tools",
    "description": "the tools for shell",
    "keywords": ["time transform", "laravel", "arithmetic expressions calculator"],
    "homepage": "https://github.com/pgogop/php-shell-tools",
    "type": "project",
    "license": "Apache-2.0",
    "support": {
        "issues": "https://github.com/pgogop/php-shell-tools/issues",
        "source": "https://github.com/pgogop/php-shell-tools"
    },
    "authors": [
        {
            "name": "Chao Xiong",
            "email": "chaoshuai.cx@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "intonate/tinker-zero": "^1.2",
        "laravel-zero/framework": "^8.8",
        "nxp/math-executor": "^2.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["cxp"]
}
